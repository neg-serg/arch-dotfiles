{
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.hoverTarget": "preview",
  "coc.preferences.jumpCommand": "drop",
  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "css",
    "markdown",
    "go",
    "json"
  ],
  "coc-actions.hideCursor": false,
  "coc.source.around.shortcut": "B",
  "coc.source.buffer.shortcut": "BS",
  "coc.source.file.ignoreHidden": false,
  "codeLens.enable": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.displayByAle": true,
  "diagnostic.errorSign": "◉",
  "diagnostic.infoSign": "◉",
  "diagnostic.messageTarget": "float",
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.virtualText": true,
  "diagnostic.warningSign": "◉",
  "list.nextKeymap": "<C-n>",
  "list.previousKeymap": "<C-p>",
  "list.source.files.defaultOptions": ["--auto-preview"],
  "list.source.outline.defaultOptions": ["--auto-preview"],
  "session.directory": "~/.config/nvim/session",
  "signature.target": "float",
  "snippets.autoTrigger": true,
  "snippets.ultisnips.directories": ["snips"],
  "snippets.ultisnips.enable": true,
  "snippets.snipmate.enable": false,
  "suggest.autoTrigger": "always",
  "suggest.detailField": "menu",
  "suggest.echodocSupport": true,
  "suggest.enablePreselect": false,
  "suggest.enablePreview": true,
  "suggest.maxCompleteItemCount": 20,
  "suggest.minTriggerInputLength": 2,
  "suggest.noselect": false,
  "suggest.removeDuplicateItems": true,
  "suggest.snippetIndicator": " ",
  "suggest.timeout": 500,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.triggerCompletionWait": 30,
  "suggest.completionItemKindLabels": {
    "module": " ",
    "function": "ﬦ",
    "variable": "\ue79b",
    "keyword": "\uf1de",
    "value": "\uf89f",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf15c",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "diagnostic-languageserver.formatFiletypes": {
    "sh": "shfmt",
    "lua": "lua-format"
  },
  "diagnostic-languageserver.formatters": {
    "shfmt": {
      "command": "shfmt",
      "args": ["-i", "2", "-bn", "-ci", "-sr"]
    }
  },
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    }
  }
}
