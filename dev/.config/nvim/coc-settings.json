{
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.hoverTarget": "preview",
  "coc.preferences.jumpCommand": "drop",
  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "css",
    "markdown",
    "go",
    "json"
  ],
  "codeLens.enable": true,
  "snippets.ultisnips.enable": true,
  "snippets.autoTrigger": true,
  "snippets.ultisnips.directories": ["snips"],
  "diagnostic.displayByAle": true,
  "diagnostic.infoSign": "◉",
  "diagnostic.refreshOnInsertMode": true,
  "list.nextKeymap": "<C-n>",
  "list.previousKeymap": "<C-p>",
  "coc.source.around.shortcut": "B",
  "coc.source.buffer.shortcut": "BS",
  "suggest.autoTrigger": "always",
  "diagnostic.virtualText": true,
  "diagnostic.warningSign": "◉",
  "diagnostic.errorSign": "◉",
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.addedSign.text": "▎",
  "git.changedSign.text": "▎",
  "git.changeRemovedSign.text": "▎",
  "git.removedSign.text": "▎",
  "git.addGBlameToBufferVar": true,
  "git.addGBlameToVirtualText": false,
  "git.branchCharacter": "\uf418",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.enableGutters": true,
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.text": "▔",
  "git.virtualTextPrefix": "❯❯",
  "suggest.detailField": "menu",
  "suggest.echodocSupport": true,
  "suggest.enablePreselect": false,
  "suggest.enablePreview": true,
  "suggest.maxCompleteItemCount": 20,
  "suggest.minTriggerInputLength": 2,
  "suggest.noselect": false,
  "suggest.removeDuplicateItems": true,
  "suggest.snippetIndicator": " ",
  "suggest.timeout": 500,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.triggerCompletionWait": 30,
  "list.source.files.defaultOptions": ["--auto-preview"],
  "list.source.outline.defaultOptions": ["--auto-preview"],
  "suggest.completionItemKindLabels": {
    "module": " ",
    "function": "ﬦ",
    "variable": "\ue79b",
    "keyword": "\uf1de",
    "value": "\uf89f",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf15c",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "session.directory": "~/.config/nvim/session",
  "coc-actions.hideCursor": false,
  "diagnostic-languageserver.formatFiletypes": {
    "sh": "shfmt",
    "lua": "lua-format"
  },
  "diagnostic-languageserver.formatters": {
    "shfmt": {
      "command": "shfmt",
      "args": ["-i", "2", "-bn", "-ci", "-sr"]
    }
  },
  "diagnostic.messageTarget": "float",
  "diagnostic.checkCurrentLine": true,
  "signature.target": "float",
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        },
        "highlight": {
          "lsRanges": true
        }
      }
    },
    "python.jediEnabled": false,
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    }
  }
}
