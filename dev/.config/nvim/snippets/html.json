{
  "/*": {
    "prefix": "/*",
    "description": "commentary",
    "body": "<!-- $1 -->"
  },
  "/**": {
    "prefix": "/**",
    "description": "commentary block",
    "body": [
      "<!-- ****************** -->",
      "<!--     $1           -->",
      "<!-- ****************** -->"
    ]
  },
  "link:css": {
    "prefix": "link:css",
    "description": "link to a css file",
    "body": "<link rel=\"stylesheet\" href=\"$1\" />"
  },
  "table": {
    "prefix": "table",
    "description": "table with caption",
    "body": [
      "<table>",
      "\t<caption>$1</caption>",
      "\t<tr>",
      "\t\t<th>$2</th>",
      "\t</tr>",
      "\t<tr>",
      "\t\t<td>$3</td>",
      "\t</tr>",
      "</table>"
    ]
  },
  "table+": {
    "prefix": "table+",
    "description": "table with head, body, foot & caption",
    "body": [
      "<table>",
      "\t<caption>$1</caption>",
      "",
      "\t<thead>",
      "\t\t<tr>",
      "\t\t\t<th>$2</th>",
      "\t\t</tr>",
      "\t</thead>",
      "\t",
      "\t<tfoot>",
      "\t\t<tr>",
      "\t\t\t<th>$2</th>",
      "\t\t</tr>",
      "\t</tfoot>",
      "",
      "\t<tbody>",
      "\t\t<tr>",
      "\t\t\t<td>$3</td>",
      "\t\t</tr>",
      "\t</tbody>",
      "</table>"
    ]
  },
  "imgl": {
    "prefix": "imgl",
    "description": "image from lorempixel",
    "body": "<img src=\"http://lorempixel.com/${1:width}/${2:height}/${3:type}\" />"
  },
  "imgl2": {
    "prefix": "imgl2",
    "description": "image from lorempicsum>",
    "body": "<img src=\"http://lorempicsum.com/${1:cartoon}/${2:width}/${3:height}/${4:index}\" />"
  }
}
