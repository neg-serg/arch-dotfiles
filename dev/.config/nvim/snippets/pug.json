{
  "doc": {
    "prefix": "doc",
    "description": "doctype html5",
    "body": "doctype html"
  },
  "var": {
    "prefix": "var",
    "description": "declare variable",
    "body": "- var $1 = $2"
  },
  "case": {
    "prefix": "case",
    "description": "case",
    "body": [
      "case $1",
      "\twhen $2",
      "\t\t$3",
      "\tdefault",
      "\t\t$4"
    ]
  },
  "if": {
    "prefix": "if",
    "description": "if/else",
    "body": [
      "if $1",
      "\t$2",
      "else",
      "\t$3"
    ]
  },
  "unl": {
    "prefix": "unl",
    "description": "unless",
    "body": [
      "unless $1",
      "\t$2"
    ]
  },
  "each": {
    "prefix": "each",
    "description": "each in",
    "body": [
      "each $1 in $2",
      "\t$3"
    ]
  },
  "for": {
    "prefix": "for",
    "description": "for in",
    "body": [
      "for $1 in $2",
      "\t$3"
    ]
  },
  "while": {
    "prefix": "while",
    "description": "while",
    "body": [
      "while $1",
      "\t$2"
    ]
  },
  "mix": {
    "prefix": "mix",
    "description": "mixin without args",
    "body": [
      "mixin $1${2:($3)}",
      "\t$4"
    ]
  },
  "ext": {
    "prefix": "ext",
    "description": "extends",
    "body": "extends $1"
  },
  "inc": {
    "prefix": "inc",
    "description": "include",
    "body": "include $1"
  },
  "incf": {
    "prefix": "incf",
    "description": "include with filter",
    "body": "include:${1:{filter\\}} ${2:{file\\}}"
  }
}
