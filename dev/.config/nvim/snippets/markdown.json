{
  "sec": {
    "prefix": "sec",
    "description": "Section",
    "body": [
      "# ${1:Section Name} #",
      "$0"
    ],
    "luasnip": {
      "priority": -50
    }
  },
  "ssec": {
    "prefix": "ssec",
    "description": "Sub Section",
    "body": [
      "## ${1:Section Name} ##",
      "$0"
    ]
  },
  "sssec": {
    "prefix": "sssec",
    "description": "SubSub Section",
    "body": "### ${1:Section Name} ### $0"
  },
  "par": {
    "prefix": "par",
    "description": "Paragraph",
    "body": [
      "#### ${1:Paragraph Name} ####",
      "$0"
    ]
  },
  "spar": {
    "prefix": "spar",
    "description": "Paragraph",
    "body": [
      "##### ${1:Paragraph Name} #####",
      "$0"
    ]
  },
  "ilc": {
    "prefix": "ilc",
    "description": "Inline Code",
    "body": "`$1`$0"
  },
  "cbl": {
    "prefix": "cbl",
    "description": "Codeblock",
    "body": [
      "```",
      "$1",
      "```",
      "$0"
    ]
  },
  "link": {
    "prefix": "link",
    "description": "link",
    "body": "[$1]($2)"
  },
  "badge": {
    "prefix": "badge",
    "description": "shield.io badge",
    "body": "![$1](https://img.shields.io/badge/${2:subject}-${3:status}-${4:color(brightscreen|green|yellowgreen|yellow|orange|red|lightgrey|blue|hex)}.svg${5:?style=${6:plastic|flat-square|social}}${7:?link=${7:http://abc.xyz&}} \"${8:}\")"
  },
  "table": {
    "prefix": "table",
    "description": "gfm table",
    "body": [
      "| ${1:...}                  | ${2:...}                  |",
      "| ------------------------- | ------------------------- |",
      "| ${3:...}                  | ${4:...}                  |"
    ]
  }
}
