{
  "if": {
    "prefix": "if",
    "description": "if",
    "body": [
      "if $1",
      "\t$2",
      "else",
      "\t$3"
    ]
  },
  "?": {
    "prefix": "?",
    "description": "if...then",
    "body": "if $1 then $2${3: else $4}"
  },
  "for": {
    "prefix": "for",
    "description": "for...in",
    "body": [
      "for $1 in $2",
      "\t$3"
    ]
  },
  "foro": {
    "prefix": "foro",
    "description": "for...of",
    "body": [
      "for $1 of $2",
      "\t$3"
    ]
  },
  "f": {
    "prefix": "f",
    "description": "function",
    "body": [
      "($1) ->",
      "\t$2"
    ]
  },
  "#": {
    "prefix": "#",
    "description": "string interpolation",
    "body": "#{$1\\}"
  }
}
