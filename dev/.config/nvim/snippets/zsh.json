{
  "!env": {
    "prefix": "!env",
    "description": "#!/usr/bin/env (!env)",
    "body": [
      "#!/usr/bin/env zsh",
      ""
    ],
    "luasnip": {
      "priority": -49
    }
  },
  "ifm": {
    "prefix": "ifm",
    "description": "if =~ ",
    "body": [
      "if [[ \\$${1:var} =~ '${2:re}' ]]; then",
      "    ${3:print \"${4:match}\"}",
      "fi"
    ]
  },
  "~": {
    "prefix": "~",
    "description": " =~ ",
    "body": "\\$${1:var} =~ '${2:re}'"
  },
  "$": {
    "prefix": "$",
    "description": "$VAR",
    "body": "\\$${1:{VAR\\}}"
  },
  "@": {
    "prefix": "@",
    "description": "${@}",
    "body": "\\${@:${1:1}\\}"
  }
}
